{
  "name": "online-workout-organiser",
  "version": "1.0.0",
  "description": "Workout plan organiser - seo friendly SPA working both online and offline",
  "repository": "https://github.com/SlimDogs/workout-organiser.git",
  "author": "Tautvydas Derzinskas",
  "license": "MIT",
  "private": false,
  "scripts": {
    "parse": "node parser.js",
    "lint": "ng lint",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --code-coverage --single-run --browsers Electron",
    "start": "ng serve --open",
    "build": "ng build",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "build:prerender": "yarn run build:client-and-server-bundles && yarn run webpack:server && yarn run generate:prerender",
    "build:ssr": "yarn run build:client-and-server-bundles && yarn run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-server": "8.2.14",
    "@angular/router": "8.2.14",
    "@nguniversal/express-engine": "8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "core-js": "3.5.0",
    "rxjs": "6.5.3",
    "sass-ultimate-boilerplate": "2.0.1",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "cheerio": "1.0.0-rc.3",
    "request": "2.88.0",
    "cz-conventional-changelog": "3.0.2",
    "image-downloader": "3.5.0",
    "@angular/cli": "8.3.20",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/jasmine": "3.5.0",
    "@types/node": "12.12.21",
    "cpy-cli": "3.0.0",
    "express": "4.17.1",
    "http-server": "0.12.0",
    "reflect-metadata": "0.1.13",
    "ts-loader": "6.2.1",
    "typescript": "3.7.3",
    "tslint": "5.20.1",
    "codelyzer": "5.2.1",
    "electron": "7.1.6",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-electron": "6.3.0",
    "karma-jasmine": "2.0.1",
    "karma-mocha-reporter": "2.2.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
